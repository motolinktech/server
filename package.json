{
  "name": "server",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "lint": "biome check --write src",
    "format": "biome format --write src",
    "start": "bun run server",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server src/server.ts",
    "check": "biome check src"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.4.2",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.13",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "elysia": "latest",
    "pg": "^8.16.3",
    "prismabox": "^1.1.26"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "bun-types": "latest",
    "prisma": "^7.2.0"
  },
  "module": "src/index.js"
}
